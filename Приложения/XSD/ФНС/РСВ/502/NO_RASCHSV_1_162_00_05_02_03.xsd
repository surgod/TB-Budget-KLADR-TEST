<?xml version="1.0" encoding="windows-1251"?>
<!-- edited with XMLSpy v2007 sp2 (http://www.altova.com) by SKS (GNIVC FNS RF) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sch="http://purl.oclc.org/dsdl/schematron" xmlns:usch="http://www.unisoftware.ru/schematron-extensions" xmlns:sql="urn:schemas-microsoft-com:mapping-schema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="Файл">
		<xs:annotation>
			<!---*******************************************************************************************************************************************************************************-->
			<xs:appinfo>
				<sch:pattern name="Проверка имени файла на соответствие значению атрибута @ИдФайл">
					<sch:rule context="Файл">
						<!--  1. Проверка имени файла на соответствие ИдФайл                                                                             -->
						<sch:assert test="usch:getFileName() = @ИдФайл">
							<usch:error code="0400400007" path="@ИдФайл" value-of-select="@ИдФайл" doc-id-path="usch:getFileName()" doc-id-caption="Файл">Имя файла обмена "<usch:value-of select="usch:getFileName()"/>" не совпадает со значением элемента ИдФайл "<usch:value-of select="@ИдФайл"/>"</usch:error>
						</sch:assert>
					</sch:rule>
				</sch:pattern>
			</xs:appinfo>
			<!---*******************************************************************************************************************************************************************************-->
			<xs:documentation>Файл обмена</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Документ">
					<xs:annotation>
						<!---*************************************************************************************************************************************************************-->
						<xs:appinfo>
							<sch:pattern name="Присутствие ФИО (руководителя организации / представителя)  в  зависимости  от  значения  ПрПодп и наличия НПЮЛ">
								<sch:rule context="Документ">
									<!-- 2. Контроль  обязательности  присутствия  ФИО (руководителя организации / представителя)  в  зависимости  от  значения  ПрПодп и наличия НПЮЛ -->
									<sch:assert test=" (Подписант/ФИО and Подписант/@ПрПодп=2) or (Подписант/ФИО and Подписант/@ПрПодп=1 and СвНП/НПЮЛ) or 	( ( (Подписант/ФИО) or  not(Подписант/ФИО) )  and Подписант/@ПрПодп=1 and СвНП/НПФЛ) ">
										<usch:error code="0400300003" path="Подписант/ФИО" value-of-select="Подписант/@ПрПодп"> Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/Подписант/ФИО при значении элемента ПрПодп, равном "<usch:value-of select="Подписант/@ПрПодп"/>". </usch:error>
									</sch:assert>
								</sch:rule>
							</sch:pattern>
						</xs:appinfo>
						<!---*************************************************************************************************************************************************************-->
						<!---*********************************************************************************************************************************************************-->
						<xs:appinfo>
							<sch:pattern name="Соответствие между значением элемента ОтчетГод и значением года из элемента ДатаДок">
								<sch:rule context="Документ">
									<!-- 4. Контроль соответствия между значением элемента ОтчетГод и значением года из элемента ДатаДок -->
									<sch:assert test=" (
									( (substring(@ОтчетГод,1,4))&gt;=2017 ) 
									and
									( (substring(@ОтчетГод,1,4))  &lt;=   (substring(@ДатаДок,7,4)) )
									)  ">
										<usch:error code="0400400007" path="@ОтчетГод" value-of-select="@ОтчетГод">Не выполнено условие к  значению элемента ОтчетГод, значение элемента ОтчетГод равно "<usch:value-of select="substring(@ОтчетГод,1,4)"/>" и значение элемента ДатаДок равно "<usch:value-of select="substring(@ДатаДок,7,4)"/>".</usch:error>
									</sch:assert>
								</sch:rule>
							</sch:pattern>
						</xs:appinfo>
						<!---*********************************************************************************************************************************************************-->
						<xs:documentation>Состав и структура документа</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="СвНП">
								<xs:annotation>
									<xs:documentation>Сведения о плательщике страховых взносов</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:element name="НПЮЛ">
											<xs:annotation>
												<!---*********************************************************************************************************************************-->
												<xs:appinfo>
													<sch:pattern name="Значение элемента КПП">
														<sch:rule context="НПЮЛ">
															<!-- 5. Контроль значения элемента КПП - 5 и 6 разряды не равны 50  -->
															<sch:assert test=" substring(@КПП,5,2)!=50 ">
																<usch:error code="0400300003" path="@КПП" value-of-select="@КПП">Не выполнено условие к значению элемента Документ/СвНП/НПЮЛ/КПП на 5 и 6 знаки, не равные 50 при значении элемента КПП, равном "<usch:value-of select="@КПП"/>".</usch:error>
															</sch:assert>
														</sch:rule>
													</sch:pattern>
												</xs:appinfo>
												<!---*********************************************************************************************************************************-->
												<xs:documentation>Плательщик страховых взносов - организация</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="СвРеоргЮЛ" minOccurs="0">
														<xs:annotation>
															<!---*********************************************************************************************************************************-->
															<xs:appinfo>
																<sch:pattern name="Значение элемента КПП">
																	<sch:rule context="СвРеоргЮЛ">
																		<!-- 6. Контроль значения элемента КПП - 5 и 6 разряды не равны 50  -->
																		<sch:assert test=" substring(@КПП,5,2)!=50 ">
																			<usch:error code="0400300003" path="@КПП" value-of-select="@КПП">Не выполнено условие к значению элемента Файл/Документ/СвНП/НПЮЛ/СвРеоргЮЛ/КПП на 5 и 6 знаки, не равные 50 при значении элемента КПП, равном "<usch:value-of select="@КПП"/>".</usch:error>
																		</sch:assert>
																	</sch:rule>
																</sch:pattern>
															</xs:appinfo>
															<!---*********************************************************************************************************************************-->
															<!---*********************************************************************************************************************************-->
															<xs:appinfo>
																<sch:pattern name="Присутствие атрибутов ИННЮЛ и КПП">
																	<sch:rule context="СвРеоргЮЛ">
																		<!-- Табл.4.5. Контроль  обязательности  присутствия  атрибутов  ИННЮЛ  и  КПП  в  зависимости  от  значения  атрибута  ФормРеорг       -->
																		<sch:assert test=" (count(@ИННЮЛ)!=0 and @ФормРеорг!=0) or (count(@ИННЮЛ)=0 and @ФормРеорг=0)  ">
																			<usch:error code="0400300003" path="@ИННЮЛ" value-of-select="@ФормРеорг">Не выполнено условие присутствия элемента Файл/Документ/СвНП/НПЮЛ/СвРеоргЮЛ/ИННЮЛ при значении элемента ФормРеорг, равном "<usch:value-of select="@ФормРеорг"/>".</usch:error>
																		</sch:assert>
																		<sch:assert test=" (count(@КПП)!=0 and @ФормРеорг!=0) or (count(@КПП)=0 and @ФормРеорг=0)  ">
																			<usch:error code="0400300003" path="@КПП" value-of-select="@ФормРеорг">Не выполнено условие присутствия элемента Файл/Документ/СвНП/НПЮЛ/СвРеоргЮЛ/@КПП при значении элемента ФормРеорг, равном "<usch:value-of select="@ФормРеорг"/>".</usch:error>
																		</sch:assert>
																	</sch:rule>
																</sch:pattern>
															</xs:appinfo>
															<!---*********************************************************************************************************************************-->
															<xs:documentation>Сведения о реорганизованной (ликвидированной) организации</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute name="ФормРеорг" use="required">
																<xs:annotation>
																	<xs:documentation>Код формы реорганизации (ликвидация) / лишения полномочий (закрытия) обособленного подразделения</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:length value="1"/>
																		<xs:enumeration value="1"/>
																		<xs:enumeration value="2"/>
																		<xs:enumeration value="3"/>
																		<xs:enumeration value="4"/>
																		<xs:enumeration value="5"/>
																		<xs:enumeration value="6"/>
																		<xs:enumeration value="7"/>
																		<xs:enumeration value="9"/>
																		<xs:enumeration value="0"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute name="ИННЮЛ" type="ИННЮЛТип" use="optional">
																<xs:annotation>
																	<xs:documentation>ИНН реорганизованной организации / лишенного полномочий (закрытого) обособленного подразделения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute name="КПП" type="КППТип" use="optional">
																<xs:annotation>
																	<xs:documentation>КПП реорганизованной организации / лишенного полномочий (закрытого) обособленного подразделения</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="НаимОрг" use="required">
													<xs:annotation>
														<xs:documentation>Наименование организации, обособленного подразделения</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="1000"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="ИННЮЛ" type="ИННЮЛТип" use="required">
													<xs:annotation>
														<xs:documentation>ИНН организации</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute name="КПП" use="required">
													<xs:annotation>
														<xs:documentation>КПП</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="КППТип"/>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="НПФЛ">
											<xs:annotation>
												<!---*************************************************************************************************************************************************************-->
												<xs:appinfo>
													<sch:pattern name="Значение элемента ИННФЛ">
														<sch:rule context="НПФЛ">
															<!-- 8. Контроль значения элемента ИННФЛ -->
															<sch:assert test="	(substring(@ИННФЛ,3,10)!=1234567890) and (substring(@ИННФЛ,3,10)!=000000000000) and (substring(@ИННФЛ,1,2)!=90) and (substring(@ИННФЛ,1,2)!=93) and (substring(@ИННФЛ,1,2)!=94) and (substring(@ИННФЛ,1,2)!=95) and (substring(@ИННФЛ,1,2)!=96) and (substring(@ИННФЛ,1,2)!=98) ">
																<usch:error code="0400400007" path="@ИННФЛ" value-of-select="@ИННФЛ"> Не выполнено условие к значению элемента Файл/Документ/СвНП/НПФЛ/ИННФЛ при значении элемента ИННФЛ, равном "<usch:value-of select="@ИННФЛ"/>". </usch:error>
															</sch:assert>
														</sch:rule>
													</sch:pattern>
												</xs:appinfo>
												<!---*************************************************************************************************************************************************************-->
												<xs:documentation>Плательщик страховых взносов - индивидуальный предприниматель, глава крестьянского (фермерского) хозяйства, физическое лицо</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="ФИО" type="ФИОТип">
														<xs:annotation>
															<xs:documentation>Фамилия, имя, отчество индивидуального предпринимателя, главы крестьянского (фермерского) хозяйства, физического лица</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="ИННФЛ" type="ИННФЛТип" use="required">
													<xs:annotation>
														<xs:documentation>ИНН физического лица</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:choice>
									<xs:attribute name="ОКВЭД" type="ОКВЭДТип" use="required">
										<xs:annotation>
											<xs:documentation>Код вида экономической деятельности по классификатору ОКВЭД2</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="Тлф" use="optional">
										<xs:annotation>
											<xs:documentation>Номер контактного телефона</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1"/>
												<xs:maxLength value="20"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="Подписант">
								<xs:annotation>
									<!---*********************************************************************************************************************************************************-->
									<xs:appinfo>
										<sch:pattern name="Присутствие сведений о представителе">
											<sch:rule context="Подписант">
												<!-- Табл.4.8. Контроль  обязательности  присутствия  Сведений  о  представителе  налогоплательщика  в  зависимости  от  значения  ПрПодп  -->
												<sch:assert test="usch:iif(@ПрПодп=2, count(СвПред)!=0, (count(СвПред)!=0 or count(СвПред)=0))">
													<usch:error code="0400300003" path="@ПрПодп" value-of-select="@ПрПодп">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/Подписант/СвПред при значении элемента ПрПодп, равном "<usch:value-of select="@ПрПодп"/>".</usch:error>
												</sch:assert>
											</sch:rule>
										</sch:pattern>
									</xs:appinfo>
									<!---*********************************************************************************************************************************************************-->
									<xs:documentation>Сведения о лице, подписавшем документ</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ФИО" type="ФИОТип" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Фамилия, имя, отчество</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="СвПред" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сведения о представителе плательщика страховых взносов</xs:documentation>
												<xs:documentation><![CDATA[Элемент обязателен при <ПрПодп>=2]]></xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="НаимДок" use="required">
													<xs:annotation>
														<xs:documentation>Наименование и реквизиты документа, подтверждающего полномочия представителя</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="120"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute name="НаимОрг" use="optional">
													<xs:annotation>
														<xs:documentation>Наименование организации - представителя плательщика</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="1000"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="ПрПодп" use="required">
										<xs:annotation>
											<xs:documentation>Признак лица, подписавшего документ</xs:documentation>
											<xs:documentation>Принимает значение:
1 – плательщик страховых взносов   |
2 – представитель плательщика страховых взносов</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:length value="1"/>
												<xs:enumeration value="1"/>
												<xs:enumeration value="2"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="РасчетСВ">
								<xs:annotation>
									<!---*********************************************************************************************************************************************************-->
									<xs:appinfo>
										<sch:pattern name="Присутствие ОбязПлатСВ в зависимости от значения ПоМесту">
											<sch:rule context="РасчетСВ">
												<!-- 12. Контроль  обязательности  присутствия элемента ОбязПлатСВ  в  зависимости  от  значения  ПоМесту  -->
												<sch:assert test="usch:iif( (../@ПоМесту!=124) and (../@ПоМесту!=240), count(ОбязПлатСВ)!=0, (count(ОбязПлатСВ)!=0 or count(ОбязПлатСВ)=0))">
													<usch:error code="0400300003" path="ОбязПлатСВ" value-of-select="../@ПоМесту">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/РасчетСВ/ОбязПлатСВ при значении элемента ПоМесту, равном "<usch:value-of select="../@ПоМесту"/>".</usch:error>
												</sch:assert>
											</sch:rule>
										</sch:pattern>
									</xs:appinfo>
									<!---*********************************************************************************************************************************************************-->
									<!---*********************************************************************************************************************************************************-->
									<xs:appinfo>
										<sch:pattern name="Присутствие ОбязПлатСВ_КФХ в зависимости от значения ПоМесту">
											<sch:rule context="РасчетСВ">
												<!-- 13. Контроль  обязательности  присутствия элемента ОбязПлатСВ_КФХ  в  зависимости  от  значения  ПоМесту  -->
												<sch:assert test="usch:iif(  (../@ПоМесту=124) or (../@ПоМесту=240) , count(ОбязПлатСВ_КФХ)!=0, count(ОбязПлатСВ_КФХ)=0)">
													<usch:error code="0400300003" path="ОбязПлатСВ" value-of-select="../@ПоМесту">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/РасчетСВ/ОбязПлатСВ_КФХ при значении элемента ПоМесту, равном "<usch:value-of select="../@ПоМесту"/>".</usch:error>
												</sch:assert>
											</sch:rule>
										</sch:pattern>
									</xs:appinfo>
									<!---*********************************************************************************************************************************************************-->
									<xs:documentation>Расчет по страховым взносам</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ОбязПлатСВ" minOccurs="0">
											<xs:annotation>
												<!---*********************************************************************************************************************************************************-->
												<xs:appinfo>
													<sch:pattern name="Присутствие элемента РасчСВ_ОПС_ОМС в зависимости от значения элемента ТипПлат">
														<sch:rule context="ОбязПлатСВ">
															<!-- Контроль  обязательности  присутствия (отсутствия) элемента  РасчСВ_ОПС_ОМС  в  зависимости  от  значения  ТипПлат  -->
															<sch:assert test="usch:iif(@ТипПлат=1, count(РасчСВ_ОПС_ОМС)!=0, (count(РасчСВ_ОПС_ОМС)!=0 or count(РасчСВ_ОПС_ОМС)=0))">
																<usch:error code="0400300003" path="РасчСВ_ОПС_ОМС" value-of-select="@ТипПлат">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/РасчетСВ/ОбязПлатСВ/РасчСВ_ОПС_ОМС при значении элемента ТипПлат, равном "<usch:value-of select="@ТипПлат"/>".</usch:error>
															</sch:assert>
														</sch:rule>
													</sch:pattern>
												</xs:appinfo>
												<!---*********************************************************************************************************************************************************-->
												<!---*********************************************************************************************************************************************************-->
												<xs:appinfo>
													<sch:pattern name="Присутствие элемента РасчСВ_ОСС.ВНМ в зависимости от значения элемента ТипПлат">
														<sch:rule context="ОбязПлатСВ">
